class Solotion{
    public:
        string longestPalindrome(string s){
            int len = s.size();
            if(len ==0 || len == 1 || (len == 2 && s[1] == s[0]){
                return string;
            }

            vector<vector<int>> obj(len , vector<int>(len));

            for(int i=0; i<len; i++){
                obj[i][i]   = 1;
            }


    }
};




